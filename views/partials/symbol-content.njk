{%-
  if members.classes
    or members.interfaces
    or members.namespaces
    or members.properties
    or members.functions
    or members.typedefs
    or members.events
%}
  <section>
    {%- from 'members-by-category.njk' import detailCategories, linkCategories, membersByCategory -%}
    {{- membersByCategory(members, 'members-links.njk', categories=linkCategories) -}}
    {{- membersByCategory(members, 'members-details.njk', categories=detailCategories) -}}
  </section>
{% endif -%}
