<pre class="source-code source-code-numbered">
  <code class="line-number">
    {%- for line in docs.split('\n') -%}
      {#- Prevent an extra blank line at the end. -#}
      {%- if not loop.last -%}
        <span id="L{{ loop.index }}">{{ loop.index }}</span>
      {% endif -%}
    {%- endfor -%}
  </code>
  {{ docs | highlight(filenameExtension, omitPre=true) }}
</pre>
